<h1> Helpmate Login Page </h1>


<%= form_for(@users, :url => helpmate_path, :html => {:method => "post"}) do |f| %>
  <%= f.label "username:" %>
  <%= text_field_tag :username %>
  <%= password_field_tag :password %>
  <%= f.submit "Login" %>
<% end %>

<a href = "/helpmate/register" > Register </a>